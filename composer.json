{
    "name": "mahbubhelal/refresh-databases",
    "description": "A package that extends Laravel's RefreshDatabase trait to support multiple database connections. Also provides fast database refreshing.",
    "type": "library",
    "version": "1.0",
    "keywords": [
        "laravel",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahbub Helal",
            "email": "mdhelal00@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "pestphp/pest": "^3.5",
        "laravel/pint": "^1.18",
        "larastan/larastan": "^3.0",
        "rector/rector": "^2.0",
        "driftingly/rector-laravel": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mahbub\\RefreshDatabases\\": "src",
            "Database\\Factories\\Mahbub\\RefreshDatabases\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mahbub\\RefreshDatabases\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mahbub\\RefreshDatabases\\RefreshDatabasesServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "./vendor/bin/pint",
        "refactor": "./vendor/bin/rector",
        "types": "./vendor/bin/phpstan",
        "test": "./vendor/bin/pest",
        "check-all": [
            "./vendor/bin/pint --test",
            "./vendor/bin/pest --coverage --min=100",
            "./vendor/bin/phpstan analyze",
            "./vendor/bin/rector --dry-run"
        ]
    }
}
